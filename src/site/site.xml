            <item name="UnitTest" href="unittest.html"/>
            <item name="Using Mirage with other Frameworks" href="integration.html"/>
            <item name="Mirage-Scala" href="mirage-scala.html"/>
            <item name="TODO" href="todo.html"/>
            <item name="Release Notes" href="release.html"/>
        </menu>
        <menu ref="reports"/>
